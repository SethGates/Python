import sqlite3

connection = sqlite3.connect('AlineFinancial.db')

with open('schema.sql') as f:
    connection.executescript(f.read())

cur = connection.cursor()

cur.execute (
     'CREATE TABLE IF NOT EXISTS users (id INTEGER PRIMARY KEY, firstname TEXT, lastname TEST, username TEXT, email TEXT, password TEXT, creditcard INTEGER)')

connection.commit()

cur.row_factory = sqlite3.Row

connection.close()